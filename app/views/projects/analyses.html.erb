<%= render 'tabs1' %>
<h2>Analyses</h2>
<table>
  <tr>
    <th>Type</th>
    <th>Number of Tests</th>
    <th>Status</th>
  </tr>
  <% if @project.analyses.count<0 %>
    <% @project.analyses.each do |an| %>
      <tr>
        <td><%=an.name %></td>
        <td>an.testx.count</td>
        <td>Not available</td>
      </tr>
    <% end %>
  <%else%>
    <tr><td><em>Empty</em></td></tr>
  <%end%>
</table>

<%=form_for([@project, @project.analyses.build]) do |p| %>
<div class="field">
   <%= p.label :'Add Analysis'%><br />
   <%= select(:name, :id, @analyses.collect {|x| [x.name]})%>
</div>
<div class="actions">
  <%=p.submit %>
</div>
<% end %>
